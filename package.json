{
    "name": "vscode-c-al",
    "displayName": "Dynamics NAV C/AL",
    "description": "Dynamics NAV C/AL programming language support for Visual Studio Code",
    "version": "0.0.8",
    "publisher": "zodiacfireworks",
    "keywords": [
        "Microsoft Dynamics",
        "Dynamics NAV",
        "C/AL",
        "syntax"
    ],
    "license": "MIT",
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#00204f",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zodiacfireworks/vscode-c-al"
    },
    "bugs": {
        "url": "https://github.com/zodiacfireworks/vscode-c-al/issues"
    },
    "homepage": "https://github.com/zodiacfireworks/vscode-c-al",
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "c-al",
            "aliases": [
                "C/AL",
                "c-al"
            ],
            "extensions": [
                ".cal",
                ".c-al",
                ".nav"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "c-al",
            "scopeName": "source.c-al",
            "path": "./syntaxes/c-al.tmLanguage.json"
        }]
    }
}
